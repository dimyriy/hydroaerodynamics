FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101350;

boundaryField
{
    ".*Walls.*"
    {
        type            zeroGradient;
    }
    outlet
    {
        type fixedValue;
        value uniform 101350;
    }
    inlet
    {
        type zeroGradient;
    }
}

