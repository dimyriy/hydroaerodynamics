FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(rho|G)"
    {
        solver          PCG;
        tolerance       1e-12;
        relTol          0;
    }

    "(p|p_rgh|p_rghFinal)"
    {
        solver           GAMG;
        tolerance        1e-8;
        relTol           0;
//        nPreSweeps       0;
//        nPostSweeps      1;
        smoother         DICGaussSeidel;
        cacheAgglomeration true;
        nCellsInCoarsestLevel 10;
        agglomerator     faceAreaPair;
        mergeLevels      1;
        maxIter          100;
    }
    pFinal
    {
        $p;
        tolerance 1e-8;
        maxIter 100;
    }
    "(U|k|omega|R|epsilon|nuTilda)"
    {
        solver smoothSolver;
        smoother      GaussSeidel;
        nSweeps       2;
        tolerance     1e-12;
        relTol        0;
        maxIter       20;
    }
    "(h|hFinal)"
    {
        solver smoothSolver;
        smoother      GaussSeidel;
        nSweeps       2;
        tolerance     1e-7;
        relTol        0;
        maxIter       20;
    }
}

SIMPLE
{
    momentumPredictor   yes;
    nNonOrthogonalCorrectors 1;
    pRefCell 0;
    pRefValue 0;
    rhoMin rhoMin [1 -3 0 0 0 0 0] 1.165;
    rhoMax rhoMax [1 -3 0 0 0 0 0] 1.185;
}
relaxationFactors
{
    p_rgh           0.3;
    rho             0.05;
    U               0.3;
    k               0.7;
    epsilon         0.7;
    h               0.5;
    omega           0.7;
    nuTilda         0.7;
}

