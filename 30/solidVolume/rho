FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "30";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 8000;

boundaryField
{
    solidWalls
    {
        type            fixedValue;
        value           uniform 8000;
    }
    solidVolume_to_fluidVolume
    {
        type            fixedValue;
        value           uniform 8000;
    }
}


