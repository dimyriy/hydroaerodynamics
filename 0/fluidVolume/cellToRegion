FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    channelWalls
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    topWalls
    {
        type            zeroGradient;
    }
    outerWalls
    {
        type            zeroGradient;
    }
    tankWalls
    {
        type            zeroGradient;
    }
    fluidVolume_to_solidVolume
    {
        type            calculated;
        value           uniform 0;
    }
}


